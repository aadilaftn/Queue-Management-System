{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["iot:Publish", "iot:Receive", "iot:PublishRetain"],
      "Resource": ["arn:aws:iot:us-east-1:028255564606:topic/queue/*"]
    },
    {
      "Effect": "Allow",
      "Action": ["iot:Subscribe"],
      "Resource": ["arn:aws:iot:us-east-1:028255564606:topicfilter/queue/*"]
    },
    {
      "Effect": "Allow",
      "Action": ["iot:Connect"],
      "Resource": ["arn:aws:iot:us-east-1:028255564606:client/*"]
    }
  ]
}
